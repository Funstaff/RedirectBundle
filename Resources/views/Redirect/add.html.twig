{% trans_default_domain 'Redirect' %}
{% extends 'FunstaffRedirectBundle::layout.html.twig' %}
{% block title %}{{ 'page.title.add'|trans }}{% endblock %}
{% block funstaff_redirect_block %}
<h1>{{ 'page.title.add'|trans }}</h1>
{{ form_start(form, {'action': path('funstaff_redirect_add'), 'method': 'POST', 'attr': {'novalidate': 'novalidate', 'class': 'uk-form uk-form-horizontal'}}) }}
{{ form_errors(form) }}
{% include 'FunstaffRedirectBundle:Redirect:form.html.twig' with {'form': form} only %}
<div class="uk-form-row">
    <input class="uk-button uk-button-primary" type="submit" value="{{ 'form.button.add'|trans() }}" />
    <a class="uk-button" href="{{ path('funstaff_redirect_index') }}">{{ 'form.button.backList'|trans }}</a>
</div>
{{ form_end(form) }}
{% endblock %}